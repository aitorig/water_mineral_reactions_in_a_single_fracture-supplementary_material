FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 0 0 0 0 0];

internalField   uniform 5e-12;

boundaryField
{
    west_surface
    {
        type            zeroGradient;
    }
    east_surface
    {
        type            zeroGradient;
    }
    north_surface
    {
        type            zeroGradient;
    }
    south_surface
    {
        type            zeroGradient;
    }
    top_surface
    {
        type            zeroGradient;
    }
    bottom_surface
    {
        type            zeroGradient;
    }
}


